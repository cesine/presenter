{
    "manifest_version": 2,
    "name": "Presenter",
    "description": "",
    "version": "0.1.0",
    "permissions": [
        "<all_urls>",
        "contextMenus",
        "downloads",
        "tabs"
    ],
    "web_accessible_resources": [
        "js/app.js",
        "js/findImages.js",
        "css/style.css"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icons/browser-action-icon19.png",
            "38": "icons/browser-action-icon38.png"
        },
        "default_title": "Presenter"
    }
}
